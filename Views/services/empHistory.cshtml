@{
	Layout = "emp";
}
@model IEnumerable<History>;
<!doctype html>
<html lang="en">
<head>
	<title>History </title>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

	<link href='https://fonts.googleapis.com/css?family=Roboto:400,100,300,700' rel='stylesheet' type='text/css'>

	<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">

	<link rel="stylesheet" href="  /serv/css/style.css" />

</head>
<body>
	<section class="ftco-section">
		<div class="container">
			<div class="row justify-content-center">
				<div class="col-md-6 text-center mb-5">
					<h2 class="heading-section">History</h2>
				</div>
			</div>
			<div class="row">
				<div class="col-md-12">
					<div class="table-wrap">
						<table class="table table-striped" style="text-align:center">
							<thead>
								<tr style="background:#3e64ff">
									<th style="color:#ffffff">HistoryID</th>
									<th style="color:#ffffff">opeID</th>
									<th style="color:#ffffff">opeName</th>
									<th style="color:#ffffff">opeDate</th>
									<th style="color:#ffffff">EgyPrice</th>
									<th style="color:#ffffff">DollerPrice</th>
									<th style="color:#ffffff">clientID</th>
									<th style="color:#ffffff">employeeID</th>
									
								</tr>
							</thead>
							<tbody>
								@{
									foreach (var item in Model)
									{
										<tr>
											<th scope="row">@item.HistoryID</th>
											<td style="margin:0;padding:0">@item.operationID</td>
											<td>@item.operationName</td>
											<td>@item.operationDate</td>
											<td>@item.egyPrice</td>
											<td>@item.dollarPrice</td>
											<td>@item.clientID</td>
											<td>@item.employeeID</td>
											
										</tr>
									}
								}


							</tbody>
						</table>
					</div>
				</div>
			</div>
		</div>
	</section>

	<script src="serv/js/jquery.min.js"></script>
	<script src="serv/js/popper.js"></script>
	<script src="serv/js/bootstrap.min.js"></script>
	<script src="serv/js/main.js"></script>

</body>
</html>
